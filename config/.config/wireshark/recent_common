# Common recent settings file for Wireshark 3.4.3.
#
# This file is regenerated each time Wireshark is quit
# and when changing configuration profile.
# So be careful, if you want to make manual changes here.

######## Recent capture files (latest last), cannot be altered through command line ########


######## Recent capture filters (latest last), cannot be altered through command line ########


######## Recent display filters (latest last), cannot be altered through command line ########

recent.display_filter: ip
recent.display_filter: not arp
recent.display_filter: eth.addr == ff:ff:ff:ff:ff:ff
recent.display_filter: ip.addr == 192.0.2.1 and not tcp.port in {80 25}
recent.display_filter: not arp and !(udp.port == 53)
recent.display_filter: http
recent.display_filter: tcp.port == 80 || udp.port == 80
recent.display_filter: !(udp.port == 53 || tcp.port == 53)
recent.display_filter: udp
recent.display_filter: tcp

